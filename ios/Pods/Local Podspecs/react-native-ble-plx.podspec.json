{
  "name": "react-native-ble-plx",
  "version": "3.1.2",
  "summary": "React Native Bluetooth Low Energy library",
  "homepage": "https://github.com/dotintent/react-native-ble-plx#readme",
  "license": "MIT",
  "authors": "dotintent (https://withintent.com/)",
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/dotintent/react-native-ble-plx.git",
    "tag": "3.1.2"
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "dependencies": {
    "MultiplatformBleAdapter": [
      "0.2.0"
    ],
    "React-Core": [

    ],
    "RCT-Folly": [
      "2022.05.16.00"
    ],
    "glog": [

    ]
  },
  "compiler_flags": "$(inherited) -DMULTIPLATFORM_BLE_ADAPTER -fmodules -fcxx-modules -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -Wno-comma -Wno-shorten-64-to-32",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
  }
}
